{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./src/resources/files.ts",
          "file_format": "qt_ts",
          "tag": "app:language.ts",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./src/resources/<language>.ts",
          "file_format": "qt_ts",
          "tag": "app:language.ts",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}